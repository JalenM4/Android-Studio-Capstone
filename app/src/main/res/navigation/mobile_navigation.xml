<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.finalproject1.ui.home.HomeFragment"
        android:label="Home" />

    <fragment
        android:id="@+id/navigation_analytics"
        android:name="com.example.finalproject1.ui.dashboard.AnalyticsFragment"
        android:label="Analytics" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.finalproject1.ui.notifications.ProfileFragment"
        android:label="Profile" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.finalproject1.ui.notifications.AccountFragment"
        android:label="Account"
        tools:layout="@layout/fragment_account" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.finalproject1.ui.notifications.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />

</navigation>
